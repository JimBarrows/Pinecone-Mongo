{
  "name" : "pinecone-mongo",
  "version" : "1.0.0",
  "description" : "The mongo database for pinceone",
  "private" : true,
  "scripts" : {
	"mongo:docker:run:dev" : "docker run --detach --name pinecone-mongo -p 27017:27017 registry.gitlab.com/reallybigtree/pinecone-mongo",
	"mongo:docker:push:dev" : "docker push registry.gitlab.com/reallybigtree/pinecone-mongo",
	"mongo:docker:build:dev" : "docker build -t registry.gitlab.com/reallybigtree/pinecone-mongo ."
  }
}
